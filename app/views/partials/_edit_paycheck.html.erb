<div class="modal fade" id="modalpaycheck<%= paycheck.id %>" tabindex="-1" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLongTitle">Modal title</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        This is my amazing modal of name <%= paycheck.name %> 
        <%= render partial: 'paychecks/form', locals: { paycheck: paycheck } %>
        <button type="button" class="btn btn-secondary float-left" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>