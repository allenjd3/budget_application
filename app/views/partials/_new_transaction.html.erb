<div class="modal fade" id="modalNewTransaction" tabindex="-1" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLongTitle">New Transaction</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <%= render partial: 'transactions/form', locals: { transaction: Transaction.new } %>
        <button type="button" class="btn btn-secondary float-left" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>